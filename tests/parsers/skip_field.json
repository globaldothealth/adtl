{
  "adtl": {
    "name": "allow-skip-field-pattern",
    "description": "Tests skipping missing fields",
    "skipFieldPattern": "flw.*",
    "tables": {
      "table": {
        "kind": "oneToOne",
        "schema": "../schemas/epoch-data.schema.json"
      }
    }
  },
  "table": {
    "id": {
      "field": "Entry_ID"
    },
    "epoch": {
      "field": "Epoch"
    },
    "text": {
      "field": "Text"
    },
    "cough": {
      "field": "cough"
    },
    "followup_cough": {
      "field": "flw_cough"
    },
    "headache": {
      "field": "headache",
      "can_skip": true
    }
  }
}