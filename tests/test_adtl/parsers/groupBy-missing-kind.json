            "description": "Sex at Birth"
        },
        "subject_id": {
            "field": "subjid",
            "description": "Subject ID"
        },
        "dataset_id": "dataset-2020-03-23",
        "country_iso3": "GBR",
        "enrolment_date": {
            "field": "dsstdat",
            "description": "Date of Enrolment"
        },
        "admission_date": {
            "field": "hostdat",
            "description": "Admission date at this facility"
        }
    }
}
