{
    "adtl": {
        "name": "groupBy",
        "description": "Example using groupBy",
        "tables": {
            "subject": {
                "groupBy": "subject_id",
                "aggregation": "lastNotNull"
            }
        }
    },
    "subject": {
        "sex_at_birth": {
            "field": "sex",
            "values": {
